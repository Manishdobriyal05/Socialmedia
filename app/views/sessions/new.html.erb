<div class="login">
   <h1>Login</h1>
   <%#= form_tag '/login' do %>
   <%= form_tag sessions_path do %>
   <%= label_tag :username %>
   <%= text_field_tag :username,nil, placeholder: "Username" %><br>
   <br>
   <%= label_tag :password %>
   <%= password_field_tag :password,nil, placeholder: "Password" %><br>
   <br>
   <%= submit_tag "Log In", class: "btn btn-primary" %>
   <%end%>
</div>
<div class="center2">
   <%= button_to "Sign In ", '/users/new', method: :get, class: "btn btn-primary"%>
   <h6>For new User</h6>
</div>